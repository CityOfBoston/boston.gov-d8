{
  "name": "drupal-composer/drupal-project",
  "description": "Project template for Drupal 8 projects with composer",
  "type": "project",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Jim Fisk",
      "role": "Drupal developer",
      "email": "james.fisk@boston.gov"
    },
    {
      "name": "David Upton",
      "role": "Drupal developer",
      "email": "david.upton@boston.gov"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "type": "package",
      "package": {
        "name": "furf/jquery-ui-touch-punch",
        "version": "1.0.0",
        "type": "drupal-library",
        "source": {
          "url": "https://github.com/furf/jquery-ui-touch-punch.git",
          "type": "git",
          "reference": "master"
        }
      }
    }
  ],
  "require": {
    "composer/installers": "^1.6",
    "cweagans/composer-patches": "^1.6",
    "drupal-composer/drupal-scaffold": "^2.5",
    "drupal/acquia_connector": "^1.14",
    "drupal/acquia_purge": "^1.0",
    "drupal/address": "^1.4",
    "drupal/admin_toolbar": "^2.0",
    "drupal/adminimal_admin_toolbar": "^1.7",
    "drupal/adminimal_theme": "^1.3",
    "drupal/advanced_text_formatter": "^1.0",
    "drupal/ape": "^1.2",
    "drupal/autologout": "^1.0",
    "drupal/better_exposed_filters": "^5.0",
    "drupal/bg_image_formatter": "^1.1",
    "drupal/chosen": "^2.5",
    "drupal/coder": "^8.0",
    "drupal/config_filter": "^1.0",
    "drupal/config_ignore": "^2.1",
    "drupal/config_split": "^1.3",
    "drupal/config_update": "1.x-dev#ac36a9f8d8a2cc730edf8cc654382715ba849adc",
    "drupal/console": "^1.0.2",
    "drupal/content_access": "^1.0",
    "drupal/contextual_range_filter": "^1.0",
    "drupal/core": "~8.8",
    "drupal/ctools": "^3.0",
    "drupal/date_popup": "^1.0",
    "drupal/date_recur": "^2.0",
    "drupal/date_recur_modular": "^2.0",
    "drupal/diff": "^1.0",
    "drupal/draggableviews": "^1.0",
    "drupal/dynamic_entity_reference": "^2.0",
    "drupal/editor_advanced_link": "^1.4",
    "drupal/editor_file": "^1.4",
    "drupal/entity": "^1.0",
    "drupal/entity_browser": "^2.0",
    "drupal/entity_clone": "^1.0",
    "drupal/entity_embed": "^1.0-beta2",
    "drupal/entity_usage": "^2.0",
    "drupal/environment_indicator": "^3.3",
    "drupal/fences": "^2.0",
    "drupal/field_display_label": "^1.0",
    "drupal/field_group": "^3.0",
    "drupal/file_entity": "^2.0",
    "drupal/geolocation": "^3.0",
    "drupal/google_tag": "^1.0",
    "drupal/honeypot": "^1.27",
    "drupal/image_url_formatter": "^1.0",
    "drupal/imagemagick": "~2.0",
    "drupal/inline_entity_form": "^1.0@beta",
    "drupal/libraries": "^3.0",
    "drupal/lightning_media": "^3.6.0",
    "drupal/lightning_workflow": "^3.6",
    "drupal/linkit": "^5.0",
    "drupal/login_security": "^1.4",
    "drupal/masquerade": "2.x-dev",
    "drupal/media_entity_browser": "^2.0",
    "drupal/memcache": "^2.0",
    "drupal/menu_block": "^1.5",
    "drupal/metatag": "^1.5",
    "drupal/moderation_sidebar": "^1.1",
    "drupal/module_filter": "^3.1",
    "drupal/office_hours": "^1.1",
    "drupal/paragraphs": "^1.3",
    "drupal/paragraphs_browser": "1.x-dev",
    "drupal/paragraphs_edit": "^2.0",
    "drupal/paranoia": "^1.0",
    "drupal/pathauto": "^1.2",
    "drupal/phpexcel": "^3.0",
    "drupal/poll": "^1.2",
    "drupal/profile": "^1.0",
    "drupal/publication_date": "^2.0",
    "drupal/purge": "^3.0",
    "drupal/rabbit_hole": "^1.0-beta5",
    "drupal/realname": "^1.0",
    "drupal/recaptcha_v3": "^1.3",
    "drupal/redirect": "^1.2",
    "drupal/rest_export_nested": "^1.0",
    "drupal/restui": "^1.16",
    "drupal/rules": "^3.0",
    "drupal/salesforce": "^4.0",
    "drupal/schema_metatag": "^1.4",
    "drupal/seckit": "^1.0",
    "drupal/shortcode": "^1.0",
    "drupal/simplesamlphp_auth": "^3.1",
    "drupal/smart_trim": "^1.1",
    "drupal/social_media": "^1.8",
    "drupal/svg_image": "^1.8",
    "drupal/token": "^1.3",
    "drupal/token_custom": "^1.0",
    "drupal/token_filter": "^1.0",
    "drupal/twig_tweak": "^2.1",
    "drupal/typed_data": "^1.0@alpha",
    "drupal/viewfield": "^3.0",
    "drupal/views_accordion": "^1.3",
    "drupal/views_bulk_operations": "^2.3",
    "drupal/votingapi": "^3.0",
    "drupal/webform": "^5.9",
    "drupal/workbench": "^1.0",
    "drupal/workflow": "^1.1_",
    "drupal/xmlsitemap": "^1.0",
    "harvesthq/chosen": "^1.8",
    "oomphinc/composer-installers-extender": "^1.1",
    "sainsburys/guzzle-oauth2-plugin": "^3.0",
    "squizlabs/php_codesniffer": "^3.5",
    "vlucas/phpdotenv": "^2.4",
    "webflo/drupal-finder": "^1.0.0",
    "webmozart/path-util": "^2.3",
    "furf/jquery-ui-touch-punch": "^1.0"
  },

  "require-dev": {
    "behat/behat": "^3.4",
    "behat/mink": "^1.7",
    "behat/mink-extension": "^2.3",
    "behat/mink-goutte-driver": "^1.2",
    "behat/mink-selenium2-driver": "^1.3",
    "drupal/config_devel": "^1.2",
    "drupal/config_installer": "^1.8",
    "drupal/devel": "^4.0",
    "drupal/drupal-extension": "^3.4",
    "drupal/migrate_plus": "^4.0",
    "drupal/migrate_source_csv": "^2.2",
    "drupal/migrate_tools": "^4.0",
    "drupal/migrate_upgrade": "^3.0",
    "drupal/stage_file_proxy": "^1.0",
    "drupal/twig_xdebug": "1.x-dev",
    "drush/drush": "^9.0",
    "fenetikm/autoload-drupal": "^0.2.0",
    "jakub-onderka/php-console-highlighter": "^0.4.0",
    "jakub-onderka/php-parallel-lint": "^1.0",
    "jarnaiz/behat-junit-formatter": "^1.3",
    "phpunit/phpunit": "*"
  },
  "conflict": {
    "drupal/core": "7.*"
  },
  "config": {
    "sort-packages": true,
    "discard-changes": true
  },
  "autoload": {
    "classmap": [
      "scripts/composer/ScriptHandler.php"
    ]
  },
  "extra": {
    "installer-types": ["library"],
    "installer-paths": {
      "docroot/core": ["type:drupal-core"],
      "docroot/libraries/{$name}": ["type:drupal-library"],
      "docroot/libraries/chosen": ["harvesthq/chosen"],
      "docroot/modules/contrib/{$name}": ["type:drupal-module"],
      "docroot/profiles/contrib/{$name}": ["type:drupal-profile"],
      "docroot/themes/contrib/{$name}": ["type:drupal-theme"],
      "drush/Commands/{$name}": ["type:drupal-drush"]
    },
    "patches": {
      "drupal/core": {
        "* CoB patch - Fix orginal node is null on migration.": "patches/cob_migration_missing_original_001.patch",
        "Optional end-date in date_range field type": "https://www.drupal.org/files/issues/2020-08-09/2794481-109.patch",
        "* Title is a render array - Fixes media library ajax popup.": "https://www.drupal.org/files/issues/2019-10-21/2663316-76.drupal.Broken-title-in-modal-dialog-when-title-is-a-render-array.patch",
        "FIX: Simple decimals fail to pass validation (https://www.drupal.org/project/drupal/issues/2230909) - DU 03-11-2020": "https://www.drupal.org/files/issues/2020-05-26/2230909-162.patch",
        "FIX: Remaining entity references can crash json module after entity is uninstalled - DU 06-01-2020": "https://www.drupal.org/files/issues/2020-06-06/2996114-141.patch"
      },
      "drupal/migrate_drupal": {
        "Fix 'Call to a member function getSetting() on null' in migrate_drupal.": "https://www.drupal.org/files/issues/2018-07-08/2984460-3.patch"
      },
      "drupal/password_policy": {
        "Patch for install of Password Policy module.": "https://www.drupal.org/files/issues/2019-05-20/password_policy-config_import_field_error-2771129-81.patch",
        "CoB patch - Reverts extension change for drupal < 8.7": "patches/fixes_unsupported_ConfigurableInterface_extension_001.patch"
      },
      "drupal/viewfield":{
        "https://www.drupal.org/project/viewfield/issues/2860523": "https://www.drupal.org/files/issues/2018-06-08/d7_upgrade_path-2860523-6.patch"
      },
      "drupal/media_entity_browser": {
        "CoB patch - Skip confirmation step": "./patches/media_entity_browser__skip_confirmation.patch"
      },
      "drupal/date_recur": {
        "CoB patch - Add helper for exdate retreival": "patches/exdate_helper.patch",
        "CoB patch - Make filter work without exposed input": "patches/makeFilterWorkWithoutExposedInput.patch"
      },
      "drupal/imagemagick": {
        "* CoB patch to implements move/copy logic": "patches/ImagemagickToolkit_identify_migrate_patch_01.patch",
        "CoB patch to reduce errors from svg processing while Acquia does not have svg libraries.": "patches/imageMagick_svg_parse.patch"
      },
      "drupal/paragraphs_edit": {
        "Lineage issue when parent not a paragraph": "https://www.drupal.org/files/issues/2019-03-22/error_on_missing_parent-2944653-13034993.patch"
      },
      "drupal/simplesamlphp_auth": {
        "https://www.drupal.org/project/simplesamlphp_auth/issues/2837029": "https://www.drupal.org/files/issues/2837029-2.patch"
      }
    },
    "autoload-drupal": {
      "modules": [
        "docroot/modules/contrib/",
        "docroot/core/modules/",
        "docroot/modules/custom/"
      ]
    }
  }
}
