{
  "name": "drupal-composer/drupal-project",
  "description": "Project template for Drupal 8 projects with composer",
  "type": "project",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Jim Fisk",
      "role": "Drupal developer",
      "email": "james.fisk@boston.gov"
    },
    {
      "name": "Keith Donaldson",
      "role": "Drupal developer",
      "email": "keith.donaldson@boston.gov"
    },
    {
      "name": "David Upton",
      "role": "Drupal developer",
      "email": "david.upton@boston.gov"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  ],
  "require": {
    "composer/installers": "^1.2",
    "cweagans/composer-patches": "^1.6",
    "drupal-composer/drupal-scaffold": "^2.5",
    "drupal/acquia_connector": "^1.14",
    "drupal/acquia_purge": "^1.0",
    "drupal/address": "^1.4",
    "drupal/admin_toolbar": "^1.23",
    "drupal/admin_toolbar_tools": "^1.23",
    "drupal/adminimal_admin_toolbar": "^1.7",
    "drupal/adminimal_theme": "^1.3",
    "drupal/advanced_text_formatter": "^1.0",
    "drupal/ape": "^1.2",
    "drupal/autologout": "^1.0",
    "drupal/better_exposed_filters": "^3.0@alpha",
    "drupal/bg_image_formatter": "^1.1",
    "drupal/chosen": "^2.5",
    "drupal/coder": "8.2.12",
    "drupal/config_filter": "^1.0",
    "drupal/config_override": "^1.0",
    "drupal/config_readonly": "^1.0",
    "drupal/config_split": "^1.3",
    "drupal/config_update": "1.x-dev",
    "drupal/console": "^1.0.2",
    "drupal/content_access": "^1.0",
    "drupal/context": "^4.0",
    "drupal/core": "^8.6",
    "drupal/cors": "^1.0",
    "drupal/ctools": "^3.0",
    "drupal/date_popup": "^1.0",
    "drupal/default_content": "^1.0",
    "drupal/diff": "^1.0",
    "drupal/draggableviews": "^1.0",
    "drupal/dynamic_entity_reference": "^2.0",
    "drupal/entity": "^1.0",
    "drupal/entity_browser": "^2.0",
    "drupal/entity_usage": "^2.0",
    "drupal/environment_indicator": "^3.3",
    "drupal/fences": "^2.0",
    "drupal/field_collection": "^1.0",
    "drupal/field_group": "^1.0",
    "drupal/file_entity": "^2.0",
    "drupal/genpass": "^1.0",
    "drupal/google_tag": "^1.0",
    "drupal/honeypot": "^1.27",
    "drupal/image_url_formatter": "^1.0",
    "drupal/imce": "^1.6",
    "drupal/inline_entity_form": "^1.0@beta",
    "drupal/libraries": "^3.0",
    "drupal/linkit": "^5.0",
    "drupal/login_security": "^1.4",
    "drupal/memcache": "^2.0",
    "drupal/menu_block": "^1.5",
    "drupal/metatag": "^1.5",
    "drupal/module_filter": "^3.1",
    "drupal/office_hours": "^1.1",
    "drupal/page_manager": "^4.0",
    "drupal/panels": "^4.3",
    "drupal/paragraphs": "^1.3",
    "drupal/paragraphs_browser": "1.x-dev",
    "drupal/paragraphs_edit": "^2.0@alpha",
    "drupal/paranoia": "^1.0",
    "drupal/password_policy": "3.x-dev",
    "drupal/pathauto": "^1.2",
    "drupal/phpexcel": "^3.0",
    "drupal/poll": "^1.2",
    "drupal/profile": "^1.0",
    "drupal/publication_date": "^2.0",
    "drupal/purge": "^3.0",
    "drupal/rabbit_hole": "^1.0",
    "drupal/realname": "^1.0",
    "drupal/redirect": "^1.2",
    "drupal/rules": "^3.0",
    "drupal/salesforce": "^3.0",
    "drupal/seckit": "^1.0",
    "drupal/shortcode": "^1.0",
    "drupal/shunt": "^2.0",
    "drupal/simplesamlphp_auth": "^3.0",
    "drupal/slack": "^1.2",
    "drupal/smart_trim": "^1.1",
    "drupal/svg_image": "^1.8",
    "drupal/token": "^1.3",
    "drupal/token_custom": "^1.0",
    "drupal/token_filter": "^1.0",
    "drupal/twig_tweak": "^2.1",
    "drupal/viewfield": "^3.0",
    "drupal/views_bulk_operations": "^2.3",
    "drupal/votingapi": "^3.0",
    "drupal/workbench": "^1.0",
    "drupal/workbench_moderation": "^1.3",
    "drupal/workflow": "^1.1_",
    "drupal/xmlsitemap": "^1.0",
    "drush/drush": "^9.0",
    "harvesthq/chosen": "^1.8",
    "oomphinc/composer-installers-extender": "^1.1",
    "phing/phing": "2.*",
    "sainsburys/guzzle-oauth2-plugin": "^3.0",
    "squizlabs/php_codesniffer": "2.*",
    "vlucas/phpdotenv": "^2.4",
    "webflo/drupal-core-require-dev": "^8.6",
    "webflo/drupal-finder": "^1.0.0",
    "webmozart/path-util": "^2.3"
  },

  "require-dev": {
    "ajgl/breakpoint-twig-extension": "^0.3.0",
    "behat/behat": "^3.4",
    "behat/mink-extension": "^2.3",
    "behat/mink-goutte-driver": "^1.2",
    "behat/mink-selenium2-driver": "^1.3",
    "behat/mink": "^1.7",
    "drupal/config_devel": "^1.2",
    "drupal/config_installer": "^1.8",
    "drupal/devel": "^1.2",
    "drupal/drupal-extension": "^3.4",
    "drupal/masquerade": "^2.0",
    "drupal/migrate_plus": "^4.0",
    "drupal/migrate_tools": "^4.0",
    "drupal/migrate_upgrade": "^3.0",
    "drupal/stage_file_proxy": "^1.0",
    "drupal/twig_xdebug": "1.x-dev",
    "jarnaiz/behat-junit-formatter": "^1.3",
    "phpunit/phpunit": "*",
    "drupal/migrate_source_csv": "^2.2",
    "webflo/drupal-core-require-dev": "^8.6"
  },
  "conflict": {
    "drupal/core": "7.*"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "classmap": [
      "scripts/composer/ScriptHandler.php"
    ]
  },
  "extra": {
    "installer-types": ["library"],
    "installer-paths": {
      "docroot/core": ["type:drupal-core"],
      "docroot/libraries/{$name}": ["type:drupal-library"],
      "docroot/libraries/chosen": ["harvesthq/chosen"],
      "docroot/modules/contrib/{$name}": ["type:drupal-module"],
      "docroot/profiles/contrib/{$name}": ["type:drupal-profile"],
      "docroot/themes/contrib/{$name}": ["type:drupal-theme"],
      "drush/Commands/{$name}": ["type:drupal-drush"]
    },
    "patches": {
       "drupal/field_group": {
        "Patch D6 field_group_migrations with file annotations.": "patches/d6_field_group.php.patch",
        "Patch D7 field_group_migrations with file annotations.": "patches/d7_field_group.php.patch"
      },
      "drupal/password_policy": {
        "Patch for install of Password Policy module.": "https://www.drupal.org/files/issues/2018-07-30/password_policy-config_import_field_error-2771129-57.patch"
      },
      "drupal/viewfield":{
        "https://www.drupal.org/project/viewfield/issues/2860523": "https://www.drupal.org/files/issues/2018-06-08/d7_upgrade_path-2860523-6.patch"
      },
      "drupal/config_update": {
        "Drush 9 support": "https://www.drupal.org/files/issues/2018-06-11/drush9-support-2974637-24.patch"
      }
    }
  }
}
