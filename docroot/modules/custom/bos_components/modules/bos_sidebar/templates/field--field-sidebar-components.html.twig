{% for item in items %}
  {# Hack to keep from wrapping the commission info in an extra
     "list-item", since itâ€™s made up of several list items.
  #}
  {% set paragraph = item.content['#paragraph'] %}
  {% if paragraph %}
    <li class="si-list-item sb-i">
      {{ item.content }}
    </li>
  {% else %}
    <li class="si-list-item sb-i"{{ attributes }}>
      {{ item }}
    </li>
  {% endif %}

{% endfor %}
